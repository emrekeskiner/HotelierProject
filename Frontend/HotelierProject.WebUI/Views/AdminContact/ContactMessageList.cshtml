@model List<InboxContactDto>
@{
    ViewData["Title"] = "ContactMessageList";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">
            @await Html.PartialAsync("/Views/AdminContact/SidebarAdminContactPartial.cshtml")
                    <div class="email-right-box">
                        
                        <div class="email-list m-t-15 mt-5">

                            @foreach (var item in Model)
                    {
                        <div class="message">
                                <a href="email-read.html">
                                    <div class="col-mail col-mail-1">
                                        <div class="email-checkbox">
                                            <input type="checkbox" id="chk19">
                                            <label class="toggle" for="chk19"></label>
                                        </div><span class="star-toggle ti-star"></span>
                                    </div>
                                    <div class="col-mail col-mail-2">
                                        <div class="subject">@item.Subject</div>
                                        <div class="date">@item.Date.ToString(" HH:mm")</div>
                                    </div>
                                </a>
                            </div>
                    }
                            

                        </div>

                    </div>
                </div>
            </div>
        </div>


